		"enzyme": "^2.8.2",
		"ima-helpers": "^0.14.0",
		"prop-types": "^15.5.10",
		"react": "^15.6.1",
		"react-addons-test-utils": "^15.6.0",
		"react-dom": "^15.6.1",
